use_frameworks!

def application_pods
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!

    # Pods for MFP
    pod 'SwiftCheck'
end

target 'LevelDB-iOS' do
    application_pods
end

target 'LevelDBTests-iOS' do
    application_pods
end

target 'LevelDB-OSX' do
    application_pods
end

target 'LevelDBTests-OSX' do
    application_pods
end
